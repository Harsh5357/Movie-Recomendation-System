            { id: 150, title: "Food, Inc.", year: 2008, genre: "documentary", rating: 7.8, director: "Robert Kenner", cast: ["Michael Pollan", "Eric Schlosser", "Richard Lobb"], plot: "An unflattering look inside America's corporate controlled food industry.", poster: "ğŸŒ½" }
        ];

        let currentGenre = 'all';
        let filteredMovies = [...movies];
        let displayedMovies = [];
        let moviesPerPage = 20;
        let currentPage = 0;

        function updateMovieCount() {
            document.getElementById('movieCount').textContent = `${movies.length} movies in our collection`;
            document.getElementById('resultCount').textContent = filteredMovies.length;
        }

        function renderMovies(moviesToRender = null, append = false) {
            const movieGrid = document.getElementById('movieGrid');
            
            if (!append) {
                movieGrid.innerHTML = '';
                displayedMovies = [];
                currentPage = 0;
            }

            const startIndex = currentPage * moviesPerPage;
            const endIndex = startIndex + moviesPerPage;
            const moviesToShow = (moviesToRender || filteredMovies).slice(startIndex, endIndex);
            
            displayedMovies = [...displayedMovies, ...moviesToShow];

            moviesToShow.forEach(movie => {
                const movieCard = document.createElement('div');
                movieCard.className = 'movie-card bg-gray-800 rounded-xl overflow-hidden cursor-pointer';
                movieCard.onclick = () => showMovieDetails(movie);
                
                movieCard.innerHTML = `
                    <div class="p-6 text-center">
                        <div class="text-6xl mb-4">${movie.poster}</div>
                        <h3 class="text-xl font-semibold mb-2">${movie.title}</h3>
                        <p class="text-gray-400 mb-2">${movie.year} â€¢ ${movie.genre.charAt(0).toUpperCase() + movie.genre.slice(1)}</p>
                        <div class="flex items-center justify-center mb-3">
                            <span class="star-rating text-lg">â˜…</span>
                            <span class="ml-1 font-medium">${movie.rating}</span>
                        </div>
                        <p class="text-sm text-gray-300 mb-4">${movie.plot.substring(0, 100)}...</p>
                        <button class="bg-purple-600 hover:bg-purple-700 px-4 py-2 rounded-lg text-sm font-medium transition-colors">
                            View Details
                        </button>
                    </div>
                `;
                
                movieGrid.appendChild(movieCard);
            });

            // Update load more button
            const loadMoreBtn = document.getElementById('loadMoreBtn');
            if (displayedMovies.length >= filteredMovies.length) {
                loadMoreBtn.style.display = 'none';
            } else {
                loadMoreBtn.style.display = 'block';
            }

            currentPage++;
        }

        function loadMoreMovies() {
            renderMovies(null, true);
        }

        function filterByGenre(genre) {
            currentGenre = genre;
            
            // Update active genre button
            document.querySelectorAll('.genre-tag').forEach(btn => {
                btn.classList.remove('active-genre', 'bg-purple-600');
                btn.classList.add('bg-gray-700');
            });
            event.target.classList.add('active-genre', 'bg-purple-600');
            event.target.classList.remove('bg-gray-700');
            
            // Filter movies
            if (genre === 'all') {
                filteredMovies = [...movies];
            } else {
                filteredMovies = movies.filter(movie => movie.genre === genre);
            }
            
            sortMovies();
            renderMovies();
            updateMovieCount();
        }

        function sortMovies() {
            const sortBy = document.getElementById('sortSelect').value;
            
            switch(sortBy) {
                case 'rating':
                    filteredMovies.sort((a, b) => b.rating - a.rating);
                    break;
                case 'year':
                    filteredMovies.sort((a, b) => b.year - a.year);
                    break;
                case 'title':
                    filteredMovies.sort((a, b) => a.title.localeCompare(b.title));
                    break;
                case 'random':
                    filteredMovies.sort(() => Math.random() - 0.5);
                    break;
            }
            
            renderMovies();
        }

        function searchMovies() {
            const searchTerm = document.getElementById('searchInput').value.toLowerCase();
            
            if (searchTerm === '') {
                filteredMovies = currentGenre === 'all' ? [...movies] : movies.filter(movie => movie.genre === currentGenre);
            } else {
                const baseMovies = currentGenre === 'all' ? movies : movies.filter(movie => movie.genre === currentGenre);
                filteredMovies = baseMovies.filter(movie => 
                    movie.title.toLowerCase().includes(searchTerm) ||
                    movie.director.toLowerCase().includes(searchTerm) ||
                    movie.cast.some(actor => actor.toLowerCase().includes(searchTerm)) ||
                    movie.plot.toLowerCase().includes(searchTerm)
                );
            }
            
            sortMovies();
            renderMovies();
            updateMovieCount();
        }

        function showMovieDetails(movie) {
            const modal = document.getElementById('movieModal');
            const modalContent = document.getElementById('modalContent');
            
            modalContent.innerHTML = `
                <div class="p-8">
                    <div class="flex justify-between items-start mb-6">
                        <div class="text-6xl">${movie.poster}</div>
                        <button onclick="closeModal()" class="text-gray-400 hover:text-white text-2xl">Ã—</button>
                    </div>
                    
                    <h2 class="text-3xl font-bold mb-2">${movie.title}</h2>
                    <p class="text-gray-400 mb-4">${movie.year} â€¢ ${movie.genre.charAt(0).toUpperCase() + movie.genre.slice(1)}</p>
                    
                    <div class="flex items-center mb-6">
                        <span class="star-rating text-xl">â˜…</span>
                        <span class="ml-2 text-lg font-medium">${movie.rating}/10</span>
                    </div>
                    
                    <div class="mb-6">
                        <h3 class="text-lg font-semibold mb-2">Director</h3>
                        <p class="text-gray-300">${movie.director}</p>
                    </div>
                    
                    <div class="mb-6">
                        <h3 class="text-lg font-semibold mb-2">Cast</h3>
                        <p class="text-gray-300">${movie.cast.join(', ')}</p>
                    </div>
                    
                    <div class="mb-8">
                        <h3 class="text-lg font-semibold mb-2">Plot</h3>
                        <p class="text-gray-300 leading-relaxed">${movie.plot}</p>
                    </div>
                    
                    <div class="flex gap-4">
                        <button class="bg-purple-600 hover:bg-purple-700 px-6 py-3 rounded-lg font-medium transition-colors">
                            Add to Watchlist
                        </button>
                        <button class="bg-gray-700 hover:bg-gray-600 px-6 py-3 rounded-lg font-medium transition-colors">
                            Mark as Watched
                        </button>
                    </div>
                </div>
            `;
            
            modal.classList.remove('hidden');
        }

        function closeModal() {
            document.getElementById('movieModal').classList.add('hidden');
        }

        // Close modal when clicking outside
        document.getElementById('movieModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeModal();
            }
        });

        // Search on Enter key
        document.getElementById('searchInput').addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                searchMovies();
            }
        });

        // Initialize the page
        document.addEventListener('DOMContentLoaded', function() {
            updateMovieCount();
            renderMovies();
        });
    </script>
